<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kibwa.campus.persistance.mapper.IRevDateMapper">

    <!-- 예약 쿼리 -->
    <select id="getRevDateList" resultType="RevDateDTO">
        select *
        from MEMBER
    </select>

    <insert id="save" parameterType="RevDateDTO">
        INSERT INTO REV (
            REV_PEOPLE,
            ENTER_DATE,
            EXIT_DATE
        )
        values(
            #{rev_People},
            #{enter_Date},
            #{exit_Date}
        )
   </insert>


</mapper>